{
  "name": "Page Notes",
  "version": "2.4.8",
  "manifest_version": 2,
  "description": "Take notes for web pages and access them with a single click whenever you visit those web pages again. (Syncs to Google Drive)",
  "background": {
     "scripts": ["aes.js", "gfile.js", "options_db.js", "pagenotes_db.js", "oauth2.js", "background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Page Notes",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon_16.png",
    "19": "icons/icon_19.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "tabs",
    "https://www.googleapis.com/",
    "https://accounts.google.com/"
   ]
}
